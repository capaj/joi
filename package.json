{
  "name": "joi",
  "description": "Object schema validation",
  "version": "5.1.1",
  "repository": "git://github.com/hapijs/joi",
  "main": "index",
  "keywords": [
    "schema",
    "validation"
  ],
  "engines": {
    "node": ">=0.10.30"
  },
  "jspm": {
    "directories": {
      "baseURL": "examples"
    },
    "dependencies": {
      "hoek-browser": "github:capaj/hoek-browser@^2.12.0",
      "moment": "2.x.x"
    }
  },
  "devDependencies": {
    "code": "1.x.x",
    "lab": "5.x.x"
  },
  "scripts": {
    "test": "node_modules/.bin/lab -t 100 -a code",
    "test-cov-html": "node_modules/.bin/lab -r html -o coverage.html -a code"
  },
  "licenses": [
    {
      "type": "BSD",
      "url": "http://github.com/hapijs/joi/raw/master/LICENSE"
    }
  ]
}
